{
  "name": "parse-epub",
  "description": "A modern and modular ePub parser",
  "main": "index.js",
  "scripts": {
    "bundle": "rollup -c .rollup/cjs.js && rollup -c .rollup/next.js",
    "push": "git tag $(npm version $VERSION) && npm publish",
    "prepublish": "npm run bundle",
    "postpublish": "rm -rf dist && git checkout package.json && git push --tags",
    "start": "jung *.js extract/*.js fetch/*.js parse/*.js -- rollup -c .rollup/playground.js & static -p 3000 playground",
    "test": "browserify --extension .es6 __tests__/index.es6 | smokestack",
    "test:one:watch": "nodemon --ext js,es6 --watch extract --exec .bin/test-one",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dariocravero/parse-epub.git"
  },
  "keywords": [
    "epub",
    "parse",
    "parser"
  ],
  "author": "Dar√≠o Javier Cravero <dario@uxtemple.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dariocravero/parse-epub/issues"
  },
  "homepage": "https://github.com/dariocravero/parse-epub#readme",
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-plugin-external-helpers": "^6.4.0",
    "babel-plugin-transform-async-to-generator": "^6.4.6",
    "babel-plugin-transform-es2015-destructuring": "^6.4.0",
    "babel-plugin-transform-export-extensions": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "browserify": "^11.2.0",
    "core-js": "^1.2.1",
    "fetch-mock": "^1.5.2",
    "jsdom": "^6.5.1",
    "node-static": "^0.7.7",
    "rollup": "0.25.1",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-npm": "^1.4.0",
    "semantic-release": "^4.3.5",
    "smokestack": "^3.4.1",
    "tape": "^4.2.1",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "lodash.uniq": "^4.0.1",
    "path-browserify": "0.0.0"
  }
}
