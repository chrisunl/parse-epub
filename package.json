{
  "name": "parse-epub",
  "description": "A modern and modular ePub parser",
  "main": "index.js",
  "scripts": {
    "build": "babel --extensions .es6 --ignore node_modules/**,__tests__/** --out-dir . .",
    "build:watch": "babel --watch --extensions .es6 --ignore node_modules/**,__tests__/** --out-dir . .",
    "clean": "find . -name '*.js*' -type f -not -path './package.json' -not -path './node_modules/*' -exec rm {} \\;",
    "prepublish": "npm run build",
    "start": "watchify --transform babelify --extension .es6 --outfile playground/app.js playground/app.es6 & static playground --cache 0 -a 0.0.0.0",
    "test": "browserify --transform babelify --extension .es6 __tests__/index.es6 | smokestack",
    "test:one:watch": "nodemon --ext js,es6 --watch extract --exec .bin/test-one"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dariocravero/parse-epub.git"
  },
  "keywords": [
    "epub",
    "parse",
    "parser"
  ],
  "author": "Dar√≠o Javier Cravero <dario@uxtemple.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dariocravero/parse-epub/issues"
  },
  "homepage": "https://github.com/dariocravero/parse-epub#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "core-js": "^1.2.1",
    "fetch-mock": "^1.5.2",
    "jsdom": "^6.5.1",
    "node-static": "^0.7.7",
    "smokestack": "^3.4.1",
    "tape": "^4.2.1",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "path-browserify": "0.0.0"
  }
}
