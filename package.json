{
  "name": "parse-epub",
  "description": "A modern and modular ePub parser",
  "main": "cjs.js",
  "scripts": {
    "build": "browserify -t [ rollupify --config rollup.config.js ] index.js > cjs.js",
    "prepublish": "npm run build",
    "start": "watchify -t [ rollupify --config rollup.config.js ] --outfile playground/app.cjs.js playground/app.js & static playground --cache 0 -a 0.0.0.0",
    "test": "browserify -t [ rollupify --config rollup.config.js ] __tests__/index.js | smokestack",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dariocravero/parse-epub.git"
  },
  "keywords": [
    "epub",
    "parse",
    "parser"
  ],
  "author": "Dar√≠o Javier Cravero <dario@uxtemple.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dariocravero/parse-epub/issues"
  },
  "homepage": "https://github.com/dariocravero/parse-epub#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "fetch-mock": "^4.3.0",
    "jsdom": "^8.2.0",
    "kneden": "^1.0.4",
    "node-static": "^0.7.7",
    "rollup-plugin-babel": "^2.4.0",
    "rollupify": "^0.2.0",
    "semantic-release": "^4.3.5",
    "smokestack": "^3.4.1",
    "tape": "^4.2.1",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "array-find": "^1.0.0",
    "mini-unique-id": "^1.0.1",
    "path-browserify": "0.0.0"
  }
}
